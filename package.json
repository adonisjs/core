{
  "name": "@adonisjs/framework",
  "version": "5.0.8",
  "description": "Adonis framework makes it easy for you to write webapps with less code",
  "main": "index.js",
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "posttest": "npm run coverage",
    "test:local": "FORCE_COLOR=true node bin/index.js --local",
    "test": "nyc japa",
    "test:win": "node ./node_modules/japa-cli/index.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "commit": "git-cz",
    "lint": "standard"
  },
  "standard": {
    "global": [
      "use"
    ]
  },
  "keywords": [
    "adonis-framework",
    "mvc",
    "mvc-framework"
  ],
  "author": "adonisjs",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/mrm-preset": "^1.0.9",
    "@adonisjs/sink": "^1.0.16",
    "argon2": "^0.19.3",
    "clear-require": "^2.0.0",
    "commitizen": "^2.10.1",
    "cookie-signature": "^1.1.0",
    "coveralls": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "fs-extra": "^6.0.1",
    "japa": "^1.0.6",
    "japa-cli": "^1.0.1",
    "mrm": "^1.2.1",
    "nyc": "^12.0.2",
    "pify": "^3.0.0",
    "pkg-ok": "^2.2.0",
    "semver": "^5.5.0",
    "simple-encryptor": "^1.4.0",
    "standard": "^11.0.1",
    "supertest": "^3.1.0",
    "test-console": "^1.1.0"
  },
  "dependencies": {
    "@adonisjs/generic-exceptions": "^2.0.1",
    "@adonisjs/middleware-base": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "co-compose": "^4.0.0",
    "dotenv": "^6.0.0",
    "edge.js": "^1.1.4",
    "eventemitter2": "^5.0.1",
    "haye": "^2.0.2",
    "lodash": "^4.17.10",
    "macroable": "^1.0.0",
    "node-cookie": "^2.1.0",
    "node-exceptions": "^3.0.0",
    "node-req": "^2.1.0",
    "node-res": "^4.1.4",
    "parseurl": "^1.3.2",
    "path-to-regexp": "^2.2.1",
    "require-all": "^3.0.0",
    "resetable": "^1.0.3",
    "serve-static": "^1.13.2",
    "useragent": "^2.3.0",
    "winston": "^3.0.0",
    "youch": "^2.0.8"
  },
  "nyc": {
    "include": [
      "src",
      "lib"
    ],
    "exclude": [
      "test",
      "japaFile.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-framework.git"
  },
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-framework/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-framework#readme",
  "bin": {
    "framework": "index.js"
  }
}
