{
  "name": "@adonisjs/dev-utils",
  "version": "1.1.0",
  "description": "Development utilities used by AdonisJs core team",
  "keywords": [
    "adonisjs",
    "devutils",
    "dev"
  ],
  "main": "build/index.js",
  "files": [
    "build/src",
    "build/index.js",
    "build/index.d.ts"
  ],
  "author": "virk",
  "homepage": "https://adonisjs.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/adonisjs/adonis-framework/tree/master/packages/dev-utils"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "nyc node japaFile.js",
    "lint": "tslint --project tsconfig.json",
    "clean": "del build",
    "doc": "typedoc",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile"
  },
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-framework/issues"
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "dependencies": {
    "clear-module": "^3.1.0",
    "fs-extra": "^7.0.1"
  }
}
