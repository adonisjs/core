{
  "name": "@adonisjs/router",
  "version": "1.5.2",
  "description": "AdonisJs official HTTP router",
  "main": "build/index.js",
  "files": [
    "build/src",
    "build/index.js",
    "build/index.d.ts",
    "build/lib"
  ],
  "scripts": {
    "pretest": "npm run lint",
    "test": "nyc node japaFile.js",
    "lint": "tslint --project tsconfig.json",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "doc": "typedoc",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "commit": "git-cz"
  },
  "keywords": [
    "router",
    "http-router"
  ],
  "author": "virk",
  "homepage": "https://adonisjs.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/adonisjs/adonis-framework/tree/master/packages/router"
  },
  "license": "MIT",
  "dependencies": {
    "@adonisjs/utils": "^1.3.2",
    "macroable": "^2.0.1",
    "matchit": "git+https://github.com/thetutlage/matchit.git",
    "object.pick": "^1.3.0",
    "pluralize": "^7.0.0"
  },
  "nyc": {
    "exclude": [
      "test-helpers",
      "test",
      "matchit"
    ],
    "extension": [
      ".ts"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
